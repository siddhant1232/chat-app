{
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "@/*": ["./*"],
      "@pages/*": ["./pages/*"]
    },
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": true,
    "jsx": "preserve",
    "types": ["vite/client"]
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}